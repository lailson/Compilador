<?xml version="1.0" encoding="UTF-8"?>

<styles>	
	<!-- NORMAL -->
    <style name="NORMAL" red="00" green="00" blue="00" bold="false"  />

	<!-- int, byte, float, etc -->
    <style name="TYPE" red="3A" green="5F" blue="CD" bold="true"  />
 	
	<!-- class, void, main, etc -->
	<style name="STRUCTURAL" red="99" green="33" blue="66" bold="true"   />
	
	<!-- coments  -->
	<style name="INVALID" red="00" green="86" blue="8b" bold="true"  />
	
	<!-- ".*"  -->
	<style name="STRING" red="CC" green="33" blue="CC" bold="false"  />
	
	<!-- 0x40, 08, 10, etc -->
	<style name="NUMBER" red="8B" green="1A" blue="1A" bold="false" />

	<!-- '.'  -->
	<style name="CHARACTER" red="99" green="99" blue="00" bold="false" />
	

</styles>